{
  "name": "mlts",
  "version": "0.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --config tslint.json --project . --format stylish",
    "test": "export RNG=`node -e 'console.log(Math.random())'` && echo $RNG && jest",
    "prepush": "npm run -s lint && npm test",
    "file": "NODE_PATH=src ts-node",
    "tsc": "tsc",
    "start": "npm run -s build; NODE_PATH=dist/src node --max-old-space-size=8192 dist/src/index.js",
    "watch": "tsc --watch"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "__TEST__": true
    },
    "testMatch": [
      "**/tests/**/*.test.ts?(x)"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^0.1.17",
    "@types/glob": "^5.0.35",
    "@types/lodash": "^4.14.108",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.0.3",
    "@types/object-hash": "^1.2.0",
    "@types/rimraf": "^2.0.2",
    "@types/seedrandom": "^2.4.27",
    "axios": "^0.18.0",
    "glob": "^7.1.3",
    "inly": "^2.0.2",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "object-hash": "^1.3.0",
    "rimraf": "^2.6.2",
    "seedrandom": "^2.4.4",
    "simplytyped": "^1.4.0",
    "tsplot": "git+https://github.com/andnp/tsplot.git",
    "utilities-ts": "git+https://github.com/andnp/utilities-ts.git",
    "validtyped": "^2.0.0"
  }
}
