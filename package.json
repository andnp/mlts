{
  "name": "mlts",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --config tslint.json --project . --format stylish",
    "test": "export RNG=`node -e 'console.log(Math.random())'` && echo $RNG && jest",
    "prepush": "npm run -s lint && npm test",
    "file": "NODE_PATH=src ts-node",
    "tsc": "tsc",
    "start": "npm run -s build; NODE_PATH=dist node --max-old-space-size=8192 dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**/*.test.ts?(x)"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/seed-random": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "seed-random": "^2.2.0",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.12.6",
    "@tensorflow/tfjs-node": "^0.1.14",
    "@types/lodash": "^4.14.108",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.0.3",
    "@types/object-hash": "^1.2.0",
    "@types/rimraf": "^2.0.2",
    "axios": "^0.18.0",
    "fast-csv": "^2.4.1",
    "inly": "^2.0.2",
    "lodash": "^4.17.10",
    "maybetyped": "^1.5.0",
    "mkdirp": "^0.5.1",
    "object-hash": "^1.3.0",
    "rimraf": "^2.6.2",
    "simplytyped": "^1.4.0",
    "type-level-schema": "^1.4.0",
    "validtyped": "^2.0.0"
  },
  "peerDependencies": {
    "@tensorflow/tfjs-node": "^0.1.14",
    "@tensorflow/tfjs-node-gpu": "^0.1.14"
  }
}
