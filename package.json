{
  "name": "mlts",
  "version": "0.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint --config tslint.json --project . --format stylish",
    "test": "export RNG=`node -e 'console.log(Math.random())'` && echo $RNG && jest tests/unit",
    "test:all": "export RNG=`node -e 'console.log(Math.random())'` && echo $RNG && jest",
    "file": "NODE_PATH=src ts-node",
    "tsc": "tsc",
    "start": "npm run -s build; NODE_PATH=dist/src node --max-old-space-size=8192 dist/scripts/test.js",
    "watch": "tsc --watch"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "__TEST__": true
    },
    "testMatch": [
      "**/tests/**/*.test.ts?(x)"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "setupFiles": []
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run -s lint && npm test"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "husky": "^2.0.0",
    "jest": "^24.0.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.9.1",
    "typescript": "~3.2.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "0.13.5",
    "@tensorflow/tfjs-node": "^0.1.21",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.108",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.7.10",
    "@types/object-hash": "^1.2.0",
    "@types/rimraf": "^2.0.2",
    "@types/seedrandom": "^2.4.27",
    "glob": "^7.1.3",
    "idx-data": "^1.0.1",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "mlts-experiment-data": "^1.0.0",
    "object-hash": "^1.3.0",
    "rimraf": "^2.6.2",
    "seedrandom": "^3.0.0",
    "simplytyped": "^3.0.0",
    "tsplot": "git+https://github.com/andnp/tsplot.git",
    "utilities-ts": "git+https://github.com/andnp/utilities-ts.git",
    "validtyped": "^2.0.0"
  }
}
